@using CoreLib.Entities
@model CResponseMessage



@{
    if (Model == null)
     {
         return;
     }




}

<script>
    toastr.options = {
        "closeButton": true,
        "debug": false,
        "newestOnTop": false,
        "progressBar": false,
        "positionClass": "toast-bottom-right",
        "preventDuplicates": false,
        "onclick": null,
        "showDuration": "300",
        "hideDuration": "1000",
        "timeOut": "20000",
        "extendedTimeOut": "1000",
        "showEasing": "swing",
        "hideEasing": "linear",
        "showMethod": "fadeIn",
        "hideMethod": "fadeOut"
    };
</script>

    @if (Model.Code == "1") // has error
    {
        <script>
            toastr.error("@Model.Message");
        </script>

        

    }
    else
    {
        <script>
            toastr.info("@Model.Message");
        </script>
    }

 